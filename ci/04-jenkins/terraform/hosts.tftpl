---
all:
  hosts:
    jenkins-master-01:
      ansible_host: ${databases["master"]["network_interface"][0]["nat_ip_address"]}
    jenkins-agent-01:
      ansible_host: ${databases["agent"]["network_interface"][0]["nat_ip_address"]}
  children:
    jenkins:
      children:
        jenkins_masters:
          hosts:
            jenkins-master-01:
        jenkins_agents:
          hosts:
              jenkins-agent-01:
  vars:
    ansible_connection_type: paramiko
    ansible_user: centos